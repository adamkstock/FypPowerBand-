<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <ScrollView android:background="@color/ti_table_background" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_weight="0.5">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <TextView android:id="@+id/fnis_compatible_images_header" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="20dp" android:layout_marginRight="20dp" android:layout_marginBottom="5dp" android:text="SUPPORTED FACTORY IMAGES" android:layout_weight="0.1"/>
            <TextView android:id="@+id/textView72" android:background="@color/ti_table_background_splitter" android:layout_width="match_parent" android:layout_height="1dp" android:layout_weight="0.1"/>
            <TableLayout android:id="@+id/fnis_supported_images" android:layout_width="match_parent" android:layout_height="match_parent" android:minHeight="50dp" android:stretchColumns="0" android:layout_weight="0.4"/>
            <TextView android:id="@+id/textView73" android:background="@color/ti_table_background_splitter" android:layout_width="match_parent" android:layout_height="1dp" android:layout_weight="0.1"/>
            <TextView android:id="@+id/fnis_uncompatible_images_header" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:layout_marginTop="20dp" android:layout_marginRight="20dp" android:layout_marginBottom="5dp" android:text="UNSUPPORTED FACTORY IMAGES" android:layout_weight="0.1"/>
            <TextView android:id="@+id/textView74" android:background="@color/ti_table_background_splitter" android:layout_width="match_parent" android:layout_height="1dp" android:layout_weight="0.1"/>
            <TableLayout android:id="@+id/fnis_unsupported_images" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginTop="5dp" android:minHeight="50dp" android:stretchColumns="0" android:layout_weight="0.4"/>
            <TextView android:id="@+id/textView75" android:background="@color/ti_table_background_splitter" android:layout_width="match_parent" android:layout_height="1dp" android:layout_weight="0.1"/>
        </LinearLayout>
    </ScrollView>
    <LinearLayout android:orientation="horizontal" android:background="@android:color/black" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_weight="0.1">
        <TextView android:textColor="@android:color/white" android:id="@+id/fnis_chip_detected_label" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="20dp" android:text="Chip detected:" android:layout_weight="0.5"/>
        <TextView android:textColor="@android:color/white" android:gravity="end" android:id="@+id/fnis_chip_id" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginRight="20dp" android:text="None detected" android:layout_weight="0.5"/>
    </LinearLayout>
</LinearLayout>
